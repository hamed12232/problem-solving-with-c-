#include <iostream>
#include <string>
using namespace std;

int main() {
	long long x, y, z,k;
	cin >> x >> y >> z>>k;
	
	if ((x + y) - z == k) {
		cout << "YES";
	}
	else if (x + y * z== k) {
		cout << "YES";
	}
	else if (x - y + z == k) {
		cout << "YES";
	}
	
	else if (x - y * z == k) {
		cout << "YES";
	}
	else if (x * y + z == k) {
		cout << "YES";
	}
	else if (x * y - z == k) {
		cout << "YES";
	}
	
	else 
		cout << "NO";
	



}
