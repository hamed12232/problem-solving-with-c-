#include <iostream>
#include <string>
using namespace std;

int main() {
	double x, y, z;
	cin >> x >> y >> z;
	double res = (x * y) / z;
	long long mynum = res;
	double c = res - mynum;
	if (c > 0) {
		cout << "double";
		//return 0;
	}
	else if (mynum <= 2147483647) {
		cout << "int";
	}
	else {
		cout << "long long";
	}
	
	



}
