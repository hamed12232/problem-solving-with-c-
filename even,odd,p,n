#include <iostream>
#include <string>
using namespace std;

int main() {
	int x;

	cin >> x;
	int even=0 ,odd=0 ,p=0 ,n = 0;
	
	for (int i = 1; i <= x; i++) {
		int num;
		cin >> num;
		if (num % 2 == 0) {
			even++;
		}
		 if (num % 2 != 0) {
			odd++;
		}
		 if (num > 0) {
			p++;
		}
		 if (num < 0) {
			n++;
		}
		
		
		
	}
	cout << "Even: " << even<<endl;
	cout << "Odd: " << odd<<endl;
	cout << "Positive: " << p<<endl;
	cout << "Negative: " << n << endl;






}
